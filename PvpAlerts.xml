				<!-- <Cooldown name="$(parent)CooldownMark" inherits="ZO_DefaultCooldown" textureFile="/esoui/art/worldmap/worldmap_map_background.dds" fillColor="FFFFFF" layer="0" level="3" hidden="false"> -->
