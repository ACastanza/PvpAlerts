		<Control name="Main_Frame_Icon_Template" mouseEnabled="true" movable="false" clampedToScreen="false" hidden="false" tier="1" layer="0" alpha="1" virtual="true">
			<OnInitialized>
				self:GetNamedChild('Cooldown'):SetDesaturation(1)
			</OnInitialized>
			<Controls>
				<Texture name="$(parent)Icon" textureFile="PapaCrown/textures/bow_3.dds" color="FFFFFF" layer="0" level="1" hidden="false">
					<Dimensions x="60" y="60" />
					<Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER" offsetY="0" />
				</Texture>
				<Texture name="$(parent)HeavyAttackHighlightIcon" textureFile="PapaCrown/textures/bow_3.dds" color="ddd36a" layer="0" level="0" hidden="false" alpha="0" scale="1.4">
					<Dimensions x="60" y="60" />
					<Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER" offsetY="0" />
				</Texture>
				<Cooldown name="$(parent)Cooldown" inherits="ZO_DefaultCooldown" textureFile="PapaCrown/textures/bow_3.dds" fillColor="787878" layer="0" level="2" hidden="false">
