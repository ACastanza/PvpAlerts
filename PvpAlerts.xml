		<TopLevelControl name="PVP_Main" mouseEnabled="true" movable="true" clampedToScreen="true" hidden="true" resizeToFitDescendents="true">
			<!-- <Dimensions x="444" y="116"/> -->
			<Anchor point="CENTER" />
			<OnMoveStop>
				PVP_Alerts_SavePosition()
			</OnMoveStop>
			<OnInitialized>
				PVP_SetupHighlightAnimation(self)
			</OnInitialized>
			<Controls>

				<Label name="$(parent)Label" text="My Label Rocks" font="$(BOLD_FONT)|28|thick-outline" horizontalAlignment="CENTER" verticalAlignment="CENTER" color="FFFFFF" wrapMode="ELLIPSIS" inheritScale="true">
					<Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER" offsetX="0"/>
					<Dimensions x="240" y="38"/>
				</Label>
				<Control name="$(parent)AbilityIconFrameLeft" inherits="Main_Frame_Icon_Template" hidden="false">
					<Dimensions x="60" y="60"/>
					<Anchor point="RIGHT" relativeTo="$(parent)Label" relativePoint="LEFT" offsetX="5" offsetY="0"/>
				</Control>
				<Control name="$(parent)AbilityIconFrameRight" inherits="Main_Frame_Icon_Template" hidden="false">
					<Dimensions x="60" y="60"/>
					<Anchor point="LEFT" relativeTo="$(parent)Label" relativePoint="RIGHT" offsetX="10" offsetY="0"/>
				</Control>
				<Texture name="$(parent)Backdrop" textureFile="EsoUI/Art/Performance/StatusMeterMunge.dds" hidden="false" excludeFromResizeToFitExtents="true">
					<TextureCoords top="0.3" bottom="0.7" left="0.15" right="0.81"/>
					<Anchor point="TOPLEFT" offsetX="-70" offsetY="-35" />
					<Anchor point="BOTTOMRIGHT" offsetX="60" offsetY="35" />
				</Texture>
				<Texture name="$(parent)Backdrop_Add" textureFile="EsoUI/Art/Performance/StatusMeterMunge.dds" hidden="false" excludeFromResizeToFitExtents="true">
					<TextureCoords top="0.3" bottom="0.7" left="0.15" right="0.81"/>
					<Anchor point="TOPLEFT" offsetX="-70" offsetY="-35" />
					<Anchor point="BOTTOMRIGHT" offsetX="60" offsetY="35" />
				</Texture>
			</Controls>
		</TopLevelControl>

		<TopLevelControl name="PVP_Counter" mouseEnabled="true" movable="true" clampedToScreen="false" hidden="true" tier="0" layer="0">
			<Dimensions x="220" y="150" />
			<Anchor point="CENTER" />
			<OnMoveStop>
				PVP_Counter_SavePosition()
			</OnMoveStop>
			<Controls>
				<Label name="$(parent)_Label" horizontalAlignment="CENTER" verticalAlignment="BOTTOM" inheritAlpha="true" color="FFFFFF" wrapMode="ELLIPSIS">
					<Dimensions x="220" y="75" />
					<Anchor point="TOP" relativeTo="$(parent)" relativePoint="TOP" offsetX="0" offsetY="-5"/>
				</Label>
				<Control name="$(parent)_CountContainer" inheritAlpha="true" resizeToFitDescendents = "true">
					<!--<DimensionConstraints minX="350" minY="50" />-->
					<Anchor point="TOP" relativeTo="$(parent)_Label" relativePoint="BOTTOM" offsetX="0" offsetY="-5"/>
					<Controls>
						<Label name="$(parent)_CountDC" text="" horizontalAlignment="CENTER" verticalAlignment="CENTER" inheritAlpha="true" color="FFFFFF" wrapMode="ELLIPSIS">
							<DimensionConstraints minX="25" minY="50" />
							<Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER" offsetX="0" offsetY="0"/>
						</Label>
						<Label name="$(parent)_SpacerADDC" text="/" horizontalAlignment="CENTER" verticalAlignment="CENTER" inheritAlpha="true" color="FFFFFF" wrapMode="ELLIPSIS">
							<DimensionConstraints minX="10" minY="50" />
							<Anchor point="TOPRIGHT" relativeTo="$(parent)_CountDC" relativePoint="TOPLEFT" offsetX="-4" offsetY="0"/>
						</Label>
						<Label name="$(parent)_CountAD" text="" horizontalAlignment="CENTER" verticalAlignment="CENTER" inheritAlpha="true" color="FFFFFF" wrapMode="ELLIPSIS">
							<DimensionConstraints minX="25" minY="50" />
							<Anchor point="BOTTOMRIGHT" relativeTo="$(parent)_SpacerADDC" relativePoint="BOTTOMLEFT" offsetX="-4" offsetY="0"/>
						</Label>
						<Label name="$(parent)_SpacerDCEP" text="/" horizontalAlignment="CENTER" verticalAlignment="CENTER" inheritAlpha="true" color="FFFFFF" wrapMode="ELLIPSIS">
							<DimensionConstraints minX="10" minY="50" />
							<Anchor point="BOTTOMLEFT" relativeTo="$(parent)_CountDC" relativePoint="BOTTOMRIGHT" offsetX="4" offsetY="0"/>
						</Label>
						<Label name="$(parent)_CountEP" text="" horizontalAlignment="CENTER" verticalAlignment="CENTER" inheritAlpha="true" color="FFFFFF" wrapMode="ELLIPSIS">
							<DimensionConstraints minX="25" minY="50" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)_SpacerDCEP" relativePoint="TOPRIGHT" offsetX="4" offsetY="0"/>
						</Label>
					</Controls>
				</Control>

				<Texture name="$(parent)_Backdrop" textureFile="EsoUI/Art/Performance/StatusMeterMunge.dds">
					<Dimensions x="225" y="400"/>
					<Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER" offsetX="0" offsetY="0"/>
				</Texture>

				<Label name="$(parent)_TestWidth" hidden="true">
					<Dimensions x="0" y="0"/>
					<Anchor point="TOP" />
				</Label>
			</Controls>
		</TopLevelControl>
		<TopLevelControl name="PVP_ForwardCamp" mouseEnabled="true" movable="true" clampedToScreen="false" hidden="true" tier="0" layer="0" alpha="1">
			<!-- <Dimensions x="50" y="50"/> -->
			<Dimensions x="80" y="80"/>

			<Anchor point="CENTER" />
			<OnMoveStop>
				PVP_Camp_SavePosition()
			</OnMoveStop>
			<Controls>
				<Texture name="$(parent)_Icon" textureFile="esoui/art/icons/mapkey/mapkey_forwardcamp.dds" layer="2">
					<!-- <Dimensions x="56" y="56"/> -->
					<Dimensions x="51" y="51"/>
					<Anchor point="CENTER" offsetX="0" offsetY="-22"/>
				</Texture>
				<Texture name="$(parent)_IconContinuous" textureFile="esoui/art/icons/ability_weapon_028.dds" layer="2">
					<!-- <Dimensions x="37" y="37"/> -->
					<Dimensions x="31" y="31"/>
					<Anchor point="CENTER" offsetX="0" offsetY="20"/>
				</Texture>
				<Texture name="$(parent)_IconAyleid" textureFile="esoui/art/icons/quest_spirit_001.dds" layer="2">
					<Dimensions x="26" y="26"/>
					<Anchor point="CENTER" offsetX="-33" offsetY="23"/>
				</Texture>
				<Texture name="$(parent)_IconBlessing" textureFile="esoui/art/icons/ability_healer_030.dds" layer="2">
					<Dimensions x="20" y="20"/>
					<Anchor point="CENTER" offsetX="32" offsetY="23"/>
				</Texture>
				<Texture name="$(parent)_Backdrop" textureFile="EsoUI/Art/Performance/StatusMeterMunge.dds" layer="0">
					<!-- <Dimensions x="120" y="300"/> -->
					<Dimensions x="176" y="500"/>
					<Anchor point="CENTER" offsetX="2" offsetY="1"/>
				</Texture>
			</Controls>
		</TopLevelControl>

		<TopLevelControl name="PVP_KillFeed" mouseEnabled="true" movable="true" clampedToScreen="false" hidden="true" tier="0" layer="0" alpha="1">
			<Dimensions x="850" y="120" />
			<Anchor point="CENTER" />
			<OnMoveStop>
				PVP_Feed_SavePosition()
			</OnMoveStop>
			<Controls>
				<TextBuffer name="$(parent)_Text" mouseEnabled="false" font="$(BOLD_FONT)|$(KB_19)|soft-shadow-thick" verticalAlignment="CENTER" text="" horizontalAlignment="CENTER" maxHistoryLines="200" inheritAlpha="true" wrapMode="ELLIPSIS" inheritScale="false">
					<Dimensions x="850" y="120" />
					<Anchor point="CENTER" offsetY="0" />
					<LineFade fadeBegin="8" fadeDuration="2"/>
					<OnMouseWheel>
						self:MoveScrollPosition(delta)
					</OnMouseWheel>
					<OnMouseEnter>
						self:ShowFadedLines()
					</OnMouseEnter>
				</TextBuffer>
				<Backdrop name="$(parent)_Ratio" hidden="true" edgeColor="444444" centerColor="333333" alpha="1">
					<Dimensions x="330" y="15" />
					<Anchor point="TOP" relativeTo="$(parent)_Text" relativePoint="BOTTOM" offsetX="0" offsetY="15" />
					<Edge edgeSize="1" />
					<Controls>
						<Backdrop name="$(parent)_AD" edgeColor="aaaaaa" centerColor="EFD13C" alpha="0.8">
							<Dimensions x="110" y="15" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="0" offsetY="0" />
							<Edge edgeSize="1" />
						</Backdrop>
						<Label name="$(parent)_AD_Label" font="$(BOLD_FONT)|$(KB_15)|soft-shadow-thin" text="AD" verticalAlignment="CENTER" horizontalAlignment="CENTER">
							<Dimensions x="50" y="25" />
							<Anchor point="CENTER" relativeTo="$(parent)_AD" relativePoint="CENTER" offsetX="0" offsetY="0" />
						</Label>
						<Backdrop name="$(parent)_DC" edgeColor="aaaaaa" centerColor="80AFFF" alpha="0.8">
							<Dimensions x="110" y="15" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)_AD" relativePoint="TOPRIGHT" offsetX="0" offsetY="0" />
							<Edge edgeSize="1" />
						</Backdrop>
						<Label name="$(parent)_DC_Label" font="$(BOLD_FONT)|$(KB_15)|soft-shadow-thin" text="DC" verticalAlignment="CENTER" horizontalAlignment="CENTER">
							<Dimensions x="50" y="25" />
							<Anchor point="CENTER" relativeTo="$(parent)_DC" relativePoint="CENTER" offsetX="0" offsetY="0" />
						</Label>
						<Backdrop name="$(parent)_EP" edgeColor="aaaaaa" centerColor="FF7161" alpha="0.8">
							<Dimensions x="110" y="15" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)_DC" relativePoint="TOPRIGHT" offsetX="0" offsetY="0" />
							<Edge edgeSize="1" />
						</Backdrop>
						<Label name="$(parent)_EP_Label" font="$(BOLD_FONT)|$(KB_15)|soft-shadow-thin" text="EP" verticalAlignment="CENTER" horizontalAlignment="CENTER">
							<Dimensions x="50" y="25" />
							<Anchor point="CENTER" relativeTo="$(parent)_EP" relativePoint="CENTER" offsetX="0" offsetY="0" />
						</Label>
					</Controls>
				</Backdrop>
				<Backdrop name="$(parent)_Backdrop" inherits="ZO_DefaultBackdrop" alpha="0.3" hidden="false" />
			</Controls>
		</TopLevelControl>

		<TopLevelControl name="PVP_Names" mouseEnabled="true" movable="true" clampedToScreen="false" hidden="true" tier="0" layer="0" alpha="1">
			<Dimensions x="240" y="500" />
			<Anchor point="CENTER" />
			<OnMoveStop>
				PVP_Names_SavePosition()
			</OnMoveStop>
			<Controls>
				<TextBuffer name="$(parent)_Text" mouseEnabled="false" font="ZoFontWinH3" verticalAlignment="CENTER" text="" horizontalAlignment="RIGHT" maxHistoryLines="200" inheritAlpha="true" linkEnabled="true" wrapMode="ELLIPSIS" inheritScale="false">
					<Dimensions x="220" y="500" />
					<Anchor point="CENTER" offsetY="0" />
					<LineFade fadeBegin="13" fadeDuration="2"/>
					<OnMouseWheel>
						self:MoveScrollPosition(delta)
					</OnMouseWheel>
				</TextBuffer>
				<Backdrop name="$(parent)_Backdrop" inherits="ZO_DefaultBackdrop" alpha="0.3" hidden="false" />
			</Controls>
		</TopLevelControl>

		<TopLevelControl name="PVP_TUG" mouseEnabled="true" movable="true" clampedToScreen="false" hidden="true" tier="0" layer="0" alpha="1">
			<Dimensions x="240" y="50" />
			<Anchor point="CENTER" offsetY="540"/>
			<OnMoveStop>
				PVP_Tug_SavePosition()
			</OnMoveStop>
			<Controls>
				<Backdrop name="$(parent)_Frame" edgeColor="ffffff" centerColor="000000" alpha="1">
					<Dimensions x="242" y="27" />
					<Anchor point="CENTER" offsetY="0"/>
					<Edge edgeSize="2" />
					<Controls>
						<Label name="$(parent)_ADbar" font="$(BOLD_FONT)|$(KB_26)|thick-outline" text="AD" verticalAlignment="CENTER" horizontalAlignment="CENTER" mouseEnabled="false" movable="false" color="ffffff">
							<Dimensions x="60" y="25" />
							<Anchor point="LEFT" offsetX="3" offsetY="-6" />
							<Controls>
								<Backdrop name="$(parent)_BG" edgeColor="aaaaaa" centerColor="C2AA49" alpha="1">
									<Dimensions x="80" y="25" />
									<Anchor point="CENTER" offsetX="-2" offsetY="6"/>
									<Edge edgeSize="1" />
								</Backdrop>
							</Controls>
						</Label>
						<Label name="$(parent)_DCbar" font="$(BOLD_FONT)|$(KB_26)|thick-outline" text="DC" verticalAlignment="CENTER" horizontalAlignment="CENTER" mouseEnabled="false" movable="false" color="ffffff">
							<Dimensions x="60" y="25" />
							<Anchor point="LEFT" relativeTo="$(parent)_ADbar" relativePoint="RIGHT" offsetX="0" offsetY="0" />
							<Controls>
								<Backdrop name="$(parent)_BG" edgeColor="aaaaaa" centerColor="4f81bd" alpha="1">
									<Dimensions x="80" y="25" />
									<Anchor point="CENTER" offsetX="-2" offsetY="6"/>
									<Edge edgeSize="1" />
								</Backdrop>
							</Controls>
						</Label>
						<Label name="$(parent)_EPbar" font="$(BOLD_FONT)|$(KB_26)|thick-outline" text="EP" verticalAlignment="CENTER" horizontalAlignment="CENTER" mouseEnabled="false" movable="false" color="ffffff">
							<Dimensions x="60" y="25" />
							<Anchor point="LEFT" relativeTo="$(parent)_DCbar" relativePoint="RIGHT" offsetX="0" offsetY="0" />
							<Controls>
								<Backdrop name="$(parent)_BG" edgeColor="aaaaaa" centerColor="de5b4e" alpha="1">
									<Dimensions x="80" y="25" />
									<Anchor point="CENTER" offsetX="-2" offsetY="6"/>
									<Edge edgeSize="1" />
								</Backdrop>
							</Controls>
						</Label>
					</Controls>
				</Backdrop>
			</Controls>
		</TopLevelControl>


		<TopLevelControl name="PVP_KOS" mouseEnabled="true" movable="true" clampedToScreen="false" hidden="true" tier="0" layer="0" alpha="1">
			<!-- <Dimensions x="300" y="370" /> -->
			<Dimensions x="350" y="360" />
			<Anchor point="CENTER" />
			<OnMoveStop>
				PVP_KOS_SavePosition()
			</OnMoveStop>
			<Controls>
				<TextBuffer name="$(parent)_Text" mouseEnabled="false" font="$(BOLD_FONT)|$(KB_17)|soft-shadow-thick" verticalAlignment="CENTER" text="" horizontalAlignment="LEFT" maxHistoryLines="200" inheritAlpha="true" linkEnabled="true" wrapMode="ELLIPSIS" inheritScale="false">
					<!-- <Dimensions x="325" y="360" /> -->
					<Dimensions x="400" y="360" />
					<Anchor point="TOP" relativeTo="$(parent)" relativePoint="TOP" offsetY="0" />
					<OnMouseWheel>
						self:MoveScrollPosition(delta)
					</OnMouseWheel>
					<Controls>
						<Backdrop name="PVP_KOS_Backdrop" inherits="ZO_DefaultBackdrop" alpha="0.8" hidden="false" />
					</Controls>
				</TextBuffer>

				<Backdrop name="$(parent)_ControlFrame" edgeColor="444444" centerColor="333333" alpha="0.8">
					<Dimensions x="350" y="8" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)_Text" relativePoint="BOTTOMLEFT" offsetX="0" offsetY="10" />
					<Edge edgeSize="1" />
					<Controls>
						<Label name="$(parent)_Button" font="$(BOLD_FONT)|$(KB_13)|soft-shadow-thin" text="Switch mode" verticalAlignment="CENTER" horizontalAlignment="CENTER" mouseEnabled="true" movable="true">
							<Dimensions x="85" y="12" />
							<Anchor point="CENTER" offsetX="0" offsetY="-2" />
							<OnMoveStart>
								PVP_KOS_Control_MoveStart(self)
							</OnMoveStart>
							<OnMoveStop>
								PVP_KOS_Control_MoveStop(self)
							</OnMoveStop>
							<Controls>
								<Backdrop name="$(parent)_BG" edgeColor="aaaaaa" centerColor="aa7777" alpha="0.8">
									<Dimensions x="85" y="16" />
									<Anchor point="CENTER" offsetY="2"/>
									<Edge edgeSize="1" />
								</Backdrop>
							</Controls>
						</Label>
					</Controls>
				</Backdrop>
			</Controls>
		</TopLevelControl>

		<TopLevelControl name="PVP_Death_Buttons" mouseEnabled="true" movable="true" clampedToScreen="false" hidden="true" tier="0" layer="0" alpha="1" resizeToFitDescendents="true">
			<Anchor point="BOTTOM" relativeTo="ZO_DeathAvAButtons" relativePoint="TOP" offsetY="-40"/>
			<Controls>
				<Control name="$(parent)Button1" inherits="ZO_KeybindButton">
					<Anchor point="TOPLEFT"/>
				</Control>
				<Control name="$(parent)Button2" inherits="ZO_KeybindButton">
					<Anchor point="TOPLEFT" relativeTo="$(parent)Button1" relativePoint="TOPRIGHT" offsetX="40"/>
				</Control>
			</Controls>
		</TopLevelControl>



