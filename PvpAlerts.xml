				<Texture name="$(parent)LeadingEdge" textureFile="EsoUI/Art/ActionBar/coolDown_leadingEdge.dds" color="ffffff" layer="0" level="3" alpha="0.65" hidden="true">
					<Dimensions x="60" y="5" />
					<Anchor point="CENTER" relativeTo="$(parent)Icon" relativePoint="BOTTOM" offsetY="0" />
					<TextureCoords left="0" right ="1" top="0.3" bottom="0.4" />
				</Texture>
				<Label name="$(parent)Label" text="" color="FFFFFF" font="ZoFontGameBold" horizontalAlignment="CENTER" verticalAlignment="CENTER">
					<Anchor point="CENTER" />
					<Dimensions x="25" y="12" />
				</Label>
				<Texture name="$(parent)Glow" textureFile="EsoUI/Art/HUD/Gamepad/gp_skillGlow.dds" layer="0" level="3" tier="HIGH" hidden="false" alpha="0">
