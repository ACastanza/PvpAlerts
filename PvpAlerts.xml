		<Control name="PVP_CaptureHeader_Template" virtual="true">
			<Controls>
				<Label name="$(parent)Label" font="$(BOLD_FONT)|$(KB_15)|thick-outline" text="Objective Name" verticalAlignment="CENTER" horizontalAlignment="LEFT" alpha="1">
					<Anchor point="TOPLEFT" offsetX="0" />
					<Dimensions x="400" y="35" />
				</Label>
